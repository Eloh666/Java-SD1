import java.util.ArrayList;

public class Year {

	private Integer yearName;
	private ArrayList<Integer> langTab;
	
	public Year(Integer yearName, ArrayList<Integer> langTab)
	{
		this.yearName = yearName;
		this.langTab = langTab;
	}

	public Integer getYearName() {
		return yearName;
	}

	public void setYearName(Integer yearName) {
		this.yearName = yearName;
	}

	public ArrayList<Integer> getLangTab() {
		return langTab;
	}

	public void setLangTab(ArrayList<Integer> langTab) {
		this.langTab = langTab;
	}
	
	public Integer getJava()
	{
		if(langTab.get(0) != null)
			return langTab.get(0);
		else
			return -1;
	}
	
	public Integer getCpp()
	{
		if(langTab.get(0) != null)
			return langTab.get(0);
		else
			return -1;
	}
	
	public Integer getCs()
	{
		if(langTab.get(0) != null)
			return langTab.get(0);
		else
			return -1;
	}
	
	public Integer getPy()
	{
		if(langTab.get(0) != null)
			return langTab.get(0);
		else
			return -1;
	}
	
	public Integer getJs()
	{
		if(langTab.get(0) != null)
			return langTab.get(0);
		else
			return -1;
	}
	
	
}
