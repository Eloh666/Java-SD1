import java.util.ArrayList;

public class TPH {

	public static void main(String[] args) 
	{

		int startValue = 40755;
		int endValue = 40755*100;
		int beginT = 285;
		int beginP = 165;
		int beginH = 143;
		int T = 0;
		int P = 0;
		int H = 0;
		Byte[] tList = new Byte[endValue];
		Byte[] pList = new Byte[endValue];
		Byte[] hList = new Byte[endValue];
		boolean flag = true;
		
		for(int i = 0; i<endValue; i++)
		{
			tList[i] = 0;
			hList[i] = 0;
			pList[i] = 0;
		}
		
		for(int i = 0; i<endValue ; i++)
		{
			beginT++;
			beginP++;
			beginH++;
			T = (beginT*(beginT + 1)/2);
			P = (beginP*(3 * beginP - 1) /2);
			H = beginH*(2 * beginH - 1);
					
			if(  T < endValue && T> 0)
			{
				System.out.println(T);
				tList[T] = 1;
			}
			
			if( P < endValue && P> 0 )
			{
				pList[P] = 1;
			}
			
			if(H  < endValue && H> 0)
			{
				System.out.println(H-startValue);
				hList[H] = 1;
			}
		}
		
		
		for(int i = 0; i < endValue && flag; i++)
		{
				if(tList[i] == 1)
					if(pList[i] == 1)
						if(hList[i] == 1)
						{
							flag = false;
							System.out.println(i+startValue);
						}
		}
		
		
		
		if(flag == true)
		System.out.println("Nada");
		System.out.println("Done");

	}

}
