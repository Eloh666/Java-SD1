import java.util.ArrayList;

public class AuxMonthsBinder {

	ArrayList<Integer> weeksIndexes;
	
	public ArrayList<Integer> getWeeksIndexes() {
		return weeksIndexes;
	}

	public void setWeeksIndexes(ArrayList<Integer> weeksIndexes) {
		this.weeksIndexes = weeksIndexes;
	}

	public AuxMonthsBinder()
	{
		weeksIndexes = new ArrayList<Integer>();
	}
}
