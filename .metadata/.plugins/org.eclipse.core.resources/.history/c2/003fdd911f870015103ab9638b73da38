import java.util.ArrayList;

public class Year {

	private Integer yearName;
	private LanguagesTab langTab;
	
	public Year(Integer yearName, LanguagesTab langTab)
	{
		this.yearName = yearName;
		this.setLangTab(langTab);
	}

	public Integer getYearName() {
		return yearName;
	}

	public void setYearName(Integer yearName) {
		this.yearName = yearName;
	}

	public LanguagesTab getLangTab() {
		return langTab;
	}

	public void setLangTab(LanguagesTab langTab) {
		this.langTab = langTab;
	}


	
}
