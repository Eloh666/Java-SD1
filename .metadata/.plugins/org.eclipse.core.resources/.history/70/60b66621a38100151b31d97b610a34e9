import java.io.BufferedReader;
import java.io.FileReader;
import java.util.TreeMap;


public class Parser {
	public static void main(String[] args)  throws Exception{
		TreeMap<String,Institution> all = new TreeMap<String,Institution>();
		BufferedReader fh =
			new BufferedReader(new FileReader("NSS_2012_FT.txt"));
		String line;
		while ((line=fh.readLine())!=null)
		{
			String lines[] = line.split("\t");
			
			if(!all.containsKey(lines[0]))
				{
					all.put(lines[0],new Institution(lines[0],lines[1]));
				}
			if(!all.get(lines[0]).getCohorts().containsKey(lines[2]))
			{
				Cohort temp = new Cohort(lines[2],Integer.parseInt(lines[15]),Integer.parseInt(lines[14]));
				all.get(lines[0]).addCohort(lines[2], temp);
			}
			int n[] = {
					Integer.parseInt(lines[5].substring(0,lines[5].length())),
					Integer.parseInt(lines[6].substring(0,lines[6].length())),
					Integer.parseInt(lines[7].substring(0,lines[7].length())),
					Integer.parseInt(lines[8].substring(0,lines[8].length())),
					Integer.parseInt(lines[9].substring(0,lines[9].length()))
					
			};
			Question temp2 = new Question(Integer.parseInt(lines[4]), n[3]+n[4],n);
			all.get(lines[0]).getCohorts().get(lines[2]).addQuestions(temp2);
		}
		fh.close();
	}

}
