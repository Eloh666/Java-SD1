import java.util.TreeMap;

public class CompTool implements java.util.Comparator<Integer[]> {
	
	public int compare(String value, TreeMap<String, Integer[]> listOne,TreeMap<String, Integer[]> listTwo)
	{
		Integer one = listOne.get(value)[1];
		Integer two = listTwo.get(value)[1];
		return -1*one.compareTo(two);
	}

}
