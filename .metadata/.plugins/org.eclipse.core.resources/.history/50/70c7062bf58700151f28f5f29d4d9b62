import java.io.BufferedReader;
import java.io.FileReader;
import java.util.ArrayList;
import java.util.TreeMap;

public class Parser {

	public static void main(String[] args) throws Exception {
		
		ArrayList<Week> weeks = new ArrayList<Week>();
		TreeMap<Integer,Year> yearsTab = new TreeMap<Integer,Year>();
		BufferedReader fh = new BufferedReader(new FileReader("iot.txt"));
		// Get rid of the first line
		String reader = fh.readLine();
		while((reader = fh.readLine())!= null)
		{
			weeks.add(new Week(reader));
		}
		System.out.println(weeks.get(0));
		fh.close();
		
		for(Integer i = 2004; i < 2016;i++)
		{
			BufferedReader fh1 = new BufferedReader(new FileReader(i+".txt"));
			// Get rid of the first line
			String yearsReader = fh1.readLine();
			yearsTab.put(i,new Year(i));
			while((yearsReader = fh1.readLine()) != null)
			{
				yearsTab.get(i).addCountry(yearsReader);
			}			
			fh1.close();
		}
		

	}

}

